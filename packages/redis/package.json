{
  "name": "@idioteque/redis",
  "version": "0.0.1",
  "description": "Redis store implementation for idioteque",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:redis": "docker-compose up -d && npm run test && docker-compose down",
    "redis:up": "docker-compose up -d",
    "redis:down": "docker-compose down",
    "redis:logs": "docker-compose logs -f redis",
    "lint": "biome lint",
    "format": "biome format --write",
    "check": "biome check",
    "clean": "rm -rf dist"
  },
  "keywords": ["redis", "store", "worker", "queue", "background-jobs"],
  "author": "Adrian Cooney <cooney.adrian@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "idioteque": "workspace:*"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.12.0",
    "ioredis": "^5.7.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
